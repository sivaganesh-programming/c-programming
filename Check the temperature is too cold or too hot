#include <stdio.h>

int main() {
    float temp;

    printf("Enter temperature: ");
    scanf("%f", &temp);

    if (temp < 15) {
        printf("Too Cold\n");
    } 
    else if (temp > 35) {
        printf("Too Hot\n");
    } 
    else {
        printf("Temperature is Normal\n");
    }

    return 0;
}
